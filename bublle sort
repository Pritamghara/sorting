// Online C++ compiler to run C++ program online
#include <iostream>
#include<algorithm>
using namespace std;

void bubblesort(int *arr,int n){
    
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            if(arr[i]>arr[j]){
                swap(arr[i],arr[j]);
            }
            
            
            
        }
    }
    
}

int main() {
    // Write C++ code here
    // cout << "Hello world!";
    
    int arr[]={2,4,1,5,3};
    int size=5;
    bubblesort(arr,size);
    for(auto i:arr){
        cout<<i;
    }
    

    return 0;
}
